# set this to the name of Azure Container Registry
acrduna=myContainerRegistry

# Ccreate an Azure Container Registry
az acr create -n acrduna -g b6duna --sku basic

# Create an AKS cluster with ACR integration
az aks create -n AKSClusterd2 -g b6duna --generate-ssh-keys --attach-acr $MYACR

az aks create -n myAKSCluster -g myResourceGroup --generate-ssh-keys --attach-acr /subscriptions/<subscription-id>/resourceGroups/myContainerRegistryResourceGroup/providers/Microsoft.ContainerRegistry/registries/myContainerRegistry