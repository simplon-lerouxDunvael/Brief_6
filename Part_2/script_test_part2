# My id Subscription
a1f74e2d-ec58-4f9a-a112-088e3469febb

# Set the name of Azure Container Registry
MYACR=acrduna

# Create an Azure Container Registry
az acr create -n $MYACR -g b6duna --sku basic

# Create an AKS cluster with ACR integration
az aks create -n AKSClusterd2 -g b6duna --enable-managed-identity --node-count 4 --enable-addons monitoring --enable-msi-auth-for-monitoring  --generate-ssh-keys --attach-acr $MYACR

